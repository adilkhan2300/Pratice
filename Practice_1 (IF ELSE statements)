//branching : If else statement
class Practice_1{
    public static void main(String[] args) {
        int AvaibableCash=100,SalePrice=20;
        
     if(AvaibableCash>SalePrice)
     System.out.println("Lemme at it");
     else 
     System.out.println("Same more money");
            
        }
    }

//branching : If else statement
class Practice_2{
    public static void main(String[] args) {
       int x=1,y,z;
       if(x<7){
           System.out.println("X is less then 7");
           y=x-5;
           z=y+x;
       }
         else{
           System.out.println("X is NOT less then 7");
           y=x+5;
           z=y-x;
       }
        }
        
    }





